{
  "name": "m21-book-search-engine",
  "version": "1.0.0",
  "description": "MERN Full Stack Book Search Application",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js --ignore client",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mateo-Wallace/M21-Book-Search-Engine.git"
  },
  "author": "Mateo Wallace",
  "license": "MIT",
    "bugs": {
    "url": "https://github.com/Mateo-Wallace/M21-Book-Search-Engine/issues"
  },
  "homepage": "https://github.com/Mateo-Wallace/M21-Book-Search-Engine#readme",
  "devDependencies": {
    "concurrently": "^5.1.0"
  }
}
